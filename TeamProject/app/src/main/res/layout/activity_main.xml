<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/Seoul"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button"
        android:backgroundTint="#00FFFFFF"
        android:onClick="RegionClick"
        android:text="서울"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/Gyeonggi"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Chungcheong"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button"
        android:backgroundTint="#00FFFFFF"
        android:onClick="RegionClick"
        android:text="충청"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/Gangwon"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Gyeonggi"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Jeolla"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button"
        android:backgroundTint="#00FFFFFF"
        android:onClick="RegionClick"
        android:text="전라"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/Jeju"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Seoul" />

    <Button
        android:id="@+id/ETC"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button"
        android:backgroundTint="#00FFFFFF"
        android:onClick="RegionClick"
        android:text="기타"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Gyeongsang"
        app:layout_constraintTop_toBottomOf="@+id/Gangwon" />

    <Button
        android:id="@+id/Gyeongsang"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button"
        android:backgroundTint="#00FFFFFF"
        android:onClick="RegionClick"
        android:text="경상"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/ETC"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Jeju"
        app:layout_constraintTop_toBottomOf="@+id/Chungcheong" />

    <Button
        android:id="@+id/Jeju"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button"
        android:backgroundTint="#00FFFFFF"
        android:onClick="RegionClick"
        android:text="제주"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/Gyeongsang"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Jeolla"
        app:layout_constraintTop_toBottomOf="@+id/Gyeonggi" />

    <Button
        android:id="@+id/Gangwon"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button"
        android:backgroundTint="#00FFFFFF"
        android:onClick="RegionClick"
        android:text="강원"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Chungcheong"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Gyeonggi"
        android:layout_width="68dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button"
        android:backgroundTint="#00FFFFFF"
        android:onClick="RegionClick"
        android:text="경기"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/Chungcheong"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Seoul"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/checkList"
        android:layout_width="145dp"
        android:layout_height="141dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="20dp"
        android:onClick="CheckOnClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Calendar"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Jeolla"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/check"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/Calendar"
        android:layout_width="145dp"
        android:layout_height="141dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="20dp"
        android:onClick="CalOnClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/checkList"
        app:layout_constraintTop_toBottomOf="@+id/ETC"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/calendar" />

    <Button
        android:id="@+id/logOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="로그아웃"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Calendar" />

</androidx.constraintlayout.widget.ConstraintLayout>